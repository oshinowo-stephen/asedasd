generator client {
    provider = "prisma-client-js"
}

datasource storage {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Player {
    id String @id
    characters Character[]
}

model Character {
    priority Int
    world String
    charURI String
    playerID String
    name String @unique 
    player Player @relation(fields: [ playerID ], references: [ id ])
}